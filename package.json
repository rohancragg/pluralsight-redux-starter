{
  "name": "pluralsight-redux-starter",
  "version": "1.0.0",
  "description": "Starter kit for React and Redux Pluralsight course by Cory House",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.*",
    "bootstrap": "3.*",
    "jquery": "3.*",
    "react": "15.*",
    "react-dom": "15.*",
    "react-redux": "5.*",
    "react-router": "3.*",
    "react-router-redux": "4.*",
    "redux": "3.*",
    "redux-thunk": "2.*",
    "toastr": "2.*"
  },
  "devDependencies": {
    "babel-cli": "6.*",
    "babel-core": "6.*",
    "babel-loader": "6.*",
    "babel-plugin-react-display-name": "2.*",
    "babel-preset-es2015": "6.*",
    "babel-preset-react": "6.*",
    "babel-preset-react-hmre": "1.*",
    "babel-register": "6.*",
    "colors": "1.*",
    "compression": "1.*",
    "cross-env": "3.*",
    "css-loader": "0.26.1",
    "enzyme": "2.*",
    "eslint": "3.*",
    "eslint-plugin-import": "2.*",
    "eslint-plugin-react": "6.*",
    "eslint-watch": "2.*",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.*",
    "express": "4.*",
    "extract-text-webpack-plugin": "1.*",
    "file-loader": "0.*",
    "jsdom": "9.*",
    "mocha": "3.*",
    "nock": "9.*",
    "npm-run-all": "3.*",
    "open": "0.0.5",
    "react-addons-test-utils": "15.*",
    "redux-immutable-state-invariant": "1.*",
    "redux-mock-store": "1.*",
    "rimraf": "2.*",
    "style-loader": "0.13.*",
    "url-loader": "0.5.*",
    "webpack": "1.*",
    "webpack-dev-middleware": "1.*",
    "webpack-hot-middleware": "2.*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coryhouse/pluralsight-redux-starter"
  }
}
